{ storage (pair (set %my_asset nat) (map %s_asset string (set nat))) ;
  parameter (nat %exec) ;
  code { UNPAIR ;
         DIP 1 { UNPAIR 2 } ;
         DUP 3 ;
         PUSH string "id0" ;
         GET ;
         IF_NONE
           { PUSH string "s_asset" ; PUSH string "ASSET_NOT_FOUND" ; PAIR ; FAILWITH }
           {} ;
         ITER { DUP 2 ;
                DUP 2 ;
                COMPARE ;
                GE ;
                IF { DUP 4 ;
                     DUP 5 ;
                     PUSH string "id0" ;
                     GET ;
                     IF_NONE
                       { PUSH string "s_asset" ; PUSH string "ASSET_NOT_FOUND" ; PAIR ; FAILWITH }
                       {} ;
                     PUSH bool False ;
                     DUP 4 ;
                     UPDATE ;
                     SOME ;
                     PUSH string "id0" ;
                     UPDATE ;
                     DIP 1 { DIG 3 ; DROP 1 } ;
                     DUG 3 }
                   {} ;
                DROP 1 } ;
         DROP 1 ;
         PAIR 2 ;
         NIL operation ;
         PAIR } }

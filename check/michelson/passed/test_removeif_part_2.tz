{ storage
    (pair (map %my_asset nat (pair (string %s) (bool %b))) (map %s_asset string (set nat))) ;
  parameter (pair %exec (nat %n) (string %is)) ;
  code { UNPAIR ;
         DIP 1 { UNPAIR 2 } ;
         UNPAIR ;
         SWAP ;
         DUP 4 ;
         PUSH string "id0" ;
         GET ;
         IF_NONE
           { PUSH string "s_asset" ; PUSH string "ASSET_NOT_FOUND" ; PAIR ; FAILWITH }
           {} ;
         ITER { DUP 4 ;
                DUP 2 ;
                GET ;
                IF_NONE
                  { PUSH string "s_asset" ; PUSH string "ASSET_NOT_FOUND" ; PAIR ; FAILWITH }
                  {} ;
                DUP 4 ;
                DUP 3 ;
                COMPARE ;
                GE ;
                IF { PUSH bool True }
                   { DUP 3 ;
                     DUP 2 ;
                     GET 1 ;
                     COMPARE ;
                     EQ ;
                     IF { DUP ; GET 2 ; NOT ; IF { PUSH bool True } { PUSH bool False } }
                        { PUSH bool False } ;
                     IF { PUSH bool True } { PUSH bool False } } ;
                IF { DUP 5 ;
                     NONE (pair string bool) ;
                     DUP 4 ;
                     UPDATE ;
                     DIP 1 { DIG 4 ; DROP 1 } ;
                     DUG 4 ;
                     DUP 6 ;
                     DUP 7 ;
                     PUSH string "id0" ;
                     GET ;
                     IF_NONE
                       { PUSH string "s_asset" ; PUSH string "ASSET_NOT_FOUND" ; PAIR ; FAILWITH }
                       {} ;
                     PUSH bool False ;
                     DUP 5 ;
                     UPDATE ;
                     SOME ;
                     PUSH string "id0" ;
                     UPDATE ;
                     DIP 1 { DIG 5 ; DROP 1 } ;
                     DUG 5 }
                   {} ;
                DROP 2 } ;
         DROP 2 ;
         PAIR 2 ;
         NIL operation ;
         PAIR } }

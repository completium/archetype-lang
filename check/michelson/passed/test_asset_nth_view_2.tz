{ storage (pair (map %my_asset string (pair (nat %v) (bool %b))) (string %res)) ;
  parameter (nat %exec) ;
  code { UNPAIR ;
         DIP 1 { UNPAIR 2 } ;
         NONE string ;
         PUSH nat 0 ;
         PAIR ;
         NIL string ;
         NIL string ;
         DUP 5 ;
         ITER { UNPAIR ;
                PUSH string "" ;
                DUP 2 ;
                COMPARE ;
                NEQ ;
                IF { DUP 3 ; DUP 2 ; CONS } { DUP 3 } ;
                DIP 1 { DIG 1 ; DROP 1 } ;
                DUG 1 ;
                DROP 2 } ;
         ITER { CONS } ;
         ITER { DUP 3 ;
                DUP 3 ;
                GET 1 ;
                COMPARE ;
                EQ ;
                IF { DUP ; SOME ; PUSH nat 1 ; DUP 4 ; GET 1 ; ADD ; PAIR }
                   { DUP 2 ; GET 2 ; PUSH nat 1 ; DUP 4 ; GET 1 ; ADD ; PAIR } ;
                SWAP ;
                DROP 2 } ;
         CDR ;
         IF_NONE { PUSH string "OUT_OF_BOUND" ; FAILWITH } {} ;
         DIP 1 { DIG 2 ; DROP 1 } ;
         DUG 2 ;
         DROP 1 ;
         PAIR 2 ;
         NIL operation ;
         PAIR } }

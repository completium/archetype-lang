{ storage nat ;
  parameter (unit %exec) ;
  code { UNPAIR ;
         DROP 1 ;
         PUSH nat 1 ;
         SOME ;
         DUP ;
         IF_NONE { PUSH nat 0 } { PUSH nat 1 ; DUP 2 ; ADD ; SWAP ; DROP 1 } ;
         DIP 1 { DIG 1 ; DROP 1 } ;
         DUG 1 ;
         PUSH nat 2 ;
         DUP 3 ;
         COMPARE ;
         EQ ;
         NOT ;
         IF { PUSH string "INVALID_VALUE" ; FAILWITH } {} ;
         DROP 1 ;
         NIL operation ;
         PAIR } }

archetype certificate_generator

variable[%transferable%] admin : role = @tz1aazS5ms5cbGkb6FN1wvWmN7yrMTTcr6wB

asset certificate identified by id {
  id : string;
  description : string;
  formula : string;
  url : string;
  hash : string;
}

action add (c : certificate) {
  called by admin

  effect {
    certificate.add(c)
  }
}

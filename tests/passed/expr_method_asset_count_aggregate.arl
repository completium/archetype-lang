archetype expr_method_asset_count_aggregate

asset o_asset identified by oid {
  oid : nat;
}

asset my_asset identified by id {
  id     : string;
  col    : aggregate<o_asset>;
} initialized by {
  {"id0"; []};
  {"id1"; []};
  {"id2"; []}
}

variable res : int = -1

entry exec () {
  res := my_asset["id0"].col.count();
}

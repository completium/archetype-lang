archetype expr_method_asset_nth_view

asset my_asset identified by id {
  id : string;
  value : int;
} initialized by {
  {"id0"; 0};
  {"id1"; 1};
  {"id2"; 2}
}

variable res : int = 0

entry exec () {
  var k ?= my_asset.head(3).nth(1) : "OUT_OF_BOUND";
  res := my_asset[k].value;
  do_require (res = 1, "ko")
}

archetype effect_method_asset_removeall_partition

asset o_asset identified by oid {
  oid : int;
}

asset my_asset identified by id {
  id     : string;
  col    : partition<o_asset>;
} initialized by {
  {"id0"; []};
  {"id1"; []};
  {"id2"; []}
}

variable res : int = 0

entry pre_exec () {
  effect {
    my_asset["id0"].col.add({1})
  }
}

entry exec () {
  effect {
    my_asset["id0"].col.removeall()
  }
}

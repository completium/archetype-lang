archetype expr_method_asset_select_one_field

asset my_asset identified by id {
  id : string;
} initialized by {
  {"id0"};
  {"id1"};
  {"id2"}
}

variable res : string = ""

entry exec () {
  specification {
    s0: res = "id2";
  }
  effect {
    var l = my_asset.select(the.id = "id2");
    var k ?= l.nth(0) : "OUT_OF_BOUND";
    res := my_asset[k].id
  }
}

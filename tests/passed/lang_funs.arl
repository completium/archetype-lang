archetype lang_funs

variable n : nat      = 0
variable i : int      = 0i
variable r : rational = 0 / 1
variable d : date     = 2020-01-01
variable e : duration = 0s
variable t : tez      = 0tz
variable s : string   = ""
variable b : bytes    = 0x00

entry exec () {
  effect {
    n := min(1, 2);
    n := max(1, 2);

    i := min(1, 2i);
    i := max(1, 2i);

    i := min(1i, 2);
    i := max(1i, 2);

    i := min(1i, 2i);
    i := max(1i, 2i);

    r := min(0.5, 1);
    r := max(0.5, 1);

    r := min(2, 0.3);
    r := max(2, 0.3);

    r := min(0.5, 1i);
    r := max(0.5, 1i);

    r := min(2i, 0.3);
    r := max(2i, 0.3);

    r := min(0.5, 0.3);
    r := max(0.5, 0.3);

    d := min(2020-01-01, 2020-12-31);
    d := max(2020-01-01, 2020-12-31);

    e := min(1h, 1s);
    e := max(1h, 1s);

    t := min(1tz, 2tz);
    t := max(1tz, 2tz);

    i := floor(1.6); // 1
    i := ceil(1.6);  // 2

    n := abs(-1);
    r := abs(-0.5);

    s := concat("abc", "def");
    s := slice("abcdef", 1, 2);
    n := length("abcdef");

    b := concat(0x12, 0xef);
    b := slice(0xabcdef01, 1, 2);
    n := length(0xabcdef01);

    s := to_string(42);
  }
}

archetype expr_method_asset_sort_view

asset my_asset identified by id {
  id : string;
  value : int;
} initialized by {
  {"id0"; 3};
  {"id1"; 2};
  {"id2"; 1}
}

variable res : int = 0

action exec () {
  specification {
    s0: res = 1;
  }
  effect {
    var l = my_asset.head(3).sort(value);
    var k = l.nth(0);
    res := my_asset[k].value
  }
}

archetype test_at

variable v int = 1

action anaction {
  specification {
    postcondition p1 {
      at(l1).a = 0 and
      at(l1).v = 1 and
      at(l2).a = 2 and
      at(l2).v = 3 and
      a = 4 and
      v = 5
    }
  }

  effect {
    var a : int = 0;
    label l1;
    a := 2;
    v := 3;
    label l2;
    a := 4;
    v := 5
  }
}

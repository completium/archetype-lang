archetype test_prec

constant a : nat = 1
constant b : nat = 2
constant c : nat = 3

variable x0 : nat = a + b + c
variable y0 : nat = a + (b + c)
variable z0 : nat = (a + b) + c

variable x1 : nat = a + b * c
variable y1 : nat = (a + b) * c
variable z1 : nat = a + (b * c)

variable x2 : nat = a * b + c
variable y2 : nat = (a * b) + c
variable z2 : nat = a * (b + c)

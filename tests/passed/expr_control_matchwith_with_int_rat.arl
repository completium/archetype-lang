archetype expr_control_matchwith_with_int_rat

enum t =
  | C1
  | C2
  | C3
  | C4
  | C5

variable res : rational = 0.0

entry exec () {
  specification {
    s0: res = 3.0;
  }
  effect {
    var x : t = C3;
    res :=
      match x with
      | C1 | C2 -> 1.0
      | C3 -> 3i
      | _ -> 2.0
      end
  }
}

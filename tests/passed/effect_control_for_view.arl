archetype effect_control_for_view

asset myasset identified by id {
  id : nat;
} initialized by {
  {1};
  {2};
  {3}
}

variable res : int = 0

entry exec () {
  specification {
    s0: res = 5;
  }
  effect {
    var views = myasset.select(id > 1);
    res := 0;
    for i in views do
      res += i;
    done
  }
}

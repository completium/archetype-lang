archetype asset_iterable_big_map_multi_expression_select

asset my_asset to iterable_big_map {
  id : string;
  value : nat = 0;
  data : bytes = 0x;
} initialized by {
  { "id0"; 0; 0x00 };
  { "id1"; 1; 0x01 };
  { "id2"; 2; 0x02 }
}

variable res : list<string> = []

entry exec () {
  res := my_asset.select(value % 2 = 0);
  // dorequire(res = ["id0"; "id2"], "ko")
}

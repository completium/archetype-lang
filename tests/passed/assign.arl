archetype assign

variable var_int : int = 0
variable var_rat : rational = 0.0
variable var_tez : tez = 0tz
variable var_dat : date = 2020-12-31
variable var_dur : duration = 0s
variable var_str : string = ""

action exec () {
  effect {
    var_int += 1;
    var_rat += 1.0;
    var_rat += 1;
    var_tez += 1tz;
    var_dur += 1d;
    var_dat += var_dur;
    var_str += "_postfix";

    var_int -= 1;
    var_rat -= 1.0;
    var_rat -= 1;
    var_tez -= 1tz;
    var_dur -= 1d;
    var_dat -= var_dur;

    var_int *= 1;
    var_rat *= 1.0;
    var_rat *= 1;
    var_tez *= 1;
    var_dur *= 1;

    var_int /= 1;
    var_rat /= 1.0;
    var_rat /= 1;
    var_tez /= 1;
    var_dur /= 1

  }
}

archetype test_iter

variable mystring string = "mystring"

asset myasset identified by str {
  str : string;
}

action tr {
  effect {
    for i in myasset do
      mystring := "newvalue"
    done;
    iter i to 10 do
      mystring := "newvalue"
    done;
    iter i from 1 to 10 do
      if (i = 0)
      then mystring := "newvalue"
    done
  }
}

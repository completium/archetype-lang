archetype effect_method_asset_removeall_aggregate

asset o_asset identified by oid {
  oid : nat;
} initialized by {
  {0};
  {1};
  {2}
}

asset my_asset identified by id {
  id     : string;
  col    : aggregate<o_asset>;
} initialized by {
  {"id0"; []};
  {"id1"; []};
  {"id2"; []}
}

variable res : int = 0

entry pre_exec () {
  effect {
    my_asset["id0"].col.add(1)
  }
}

entry exec () {
  effect {
    my_asset["id0"].col.removeall()
  }
}

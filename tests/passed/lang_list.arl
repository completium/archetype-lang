archetype lang_list

entry exec () {
  effect {
    var l : list<string> = ["1"; "2"; "3"];

    (* expr *)
    var a  : list<string>            = prepend(l, "2");
    var ht : (string * list<string>) = head_tail(l);
    var k  : nat                     = length(l);
    var c  : bool                    = contains(l, "2");
    var n  : string                  = nth(l, 0);
    var r  : list<string>            = reverse(l);
    var ll : list<string>            = concat(l, l);
  }
}

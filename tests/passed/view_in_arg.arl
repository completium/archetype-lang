archetype view_in_arg

asset my_asset identified by id {
  id : string;
  value : int;
} initialized by {
  {"id0"; 0};
  {"id1"; 1};
  {"id2"; 2}
}

variable res : nat = 0

function f (v : view<my_asset>) : nat {
  return v.count()
}

entry exec () {
  specification {
    s0: res = 2;
  }
  effect {
    var v = my_asset.select(value > 0);
    res := f(v);
  }
}

archetype test_asset_update_partition_5

asset o_asset {
  oid : nat;
  v   : string;
} initialized by {
  {0; "str0"};
  {1; "str1"};
  {2; "str2"};
  {3; "str3"};
  {4; "str4"}
}

asset my_asset identified by id {
  id : string;
  v0 : nat;
  c  : partition<o_asset>;
} initialized by {
  {"id0"; 0; []};
  {"id1"; 1; []};
  {"id2"; 2; []}
}

entry exec (n : nat) {
  my_asset.update("id1", {c += [{oid = 5; v = "str5"}; {oid = n; v = "strn"}]})
}

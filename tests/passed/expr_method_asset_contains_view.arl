archetype expr_method_asset_contains_view

asset my_asset identified by id {
  id : string;
  value : int;
} initialized by {
  {"id0"; 0};
  {"id1"; 1};
  {"id2"; 2}
}

variable res : bool = false

entry exec () {
  specification {
    s0: res = true;
  }
  effect {
    res := my_asset.select(value = 0).contains("id0")
  }
}

archetype method_in_dorequire_or_dofailif

asset my_asset identified by id {
  id : string;
  value : int;
} initialized by {
  {"id0"; 0};
  {"id1"; 1};
  {"id2"; 2}
}

variable res : int = 0

entry exec (str : string) {
  effect {
    do_require(my_asset["id0"].value = 1, "ko");
    do_fail_if(my_asset["id0"].value = 1, "ko");
  }
}

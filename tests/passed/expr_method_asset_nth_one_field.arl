archetype expr_method_asset_nth_one_field

asset my_asset identified by id {
  id : string;
} initialized by {
  {"id0"};
  {"id1"};
  {"id2"}
}

variable res : string = ""

entry exec () {
  specification {
    s0: res = "id1";
  }
  effect {
    var k ?= my_asset.nth(1) : "OUT_OF_BOUND";
    res := my_asset[k].id;
    do_require (res = "id1", "ko")
  }
}

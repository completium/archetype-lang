archetype effect_method_asset_add_partition_one_field

asset o_asset identified by oid {
  oid : string;
}

asset my_asset identified by id {
  id     : string;
  value  : int;
  col    : o_asset partition;
} initialized by {
  {"id0"; 0; []};
  {"id1"; 1; []};
  {"id2"; 2; []}
}

variable res : int = 0

entry exec () {
  effect {
    my_asset["id0"].col.add({"oid"})
  }
}

archetype lang_set

entry exec () {
  effect {
    var my_set : set<nat> = [0; 1 ; 2; 3];

    (* expr *)
    var new_set2 : set<nat> = add (my_set, 4);
    var new_set3 : set<nat> = remove(my_set, 0);
    var set_c    : bool     = contains(my_set, 2);
    var c        : int      = length(my_set);

    (* control *)
    var res : int = 0;
    for v in my_set do
      res += v;
    done;
    dorequire (res = 6, "ko")
  }
}

archetype select_with_extra_var

asset my_asset identified by id {
  id : string;
  value : int;
} initialized by {
  {"id0"; 0};
  {"id1"; 1};
  {"id2"; 2}
}

variable res : int = 0

action exec (str : string) {
  specification {
    s0: res = 2;
  }
  effect {
    var l = my_asset.select(the.id = str);
    var a = l.nth(0);
    res := a.value
  }
}

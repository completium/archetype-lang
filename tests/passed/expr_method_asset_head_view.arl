archetype expr_method_asset_head_view

asset my_asset identified by id {
  id : string;
  value : int;
} initialized by {
  {"id0"; 4};
  {"id1"; 3};
  {"id2"; 2}
}

variable res : int = 0

entry exec () {
  specification {
    s0: res = 3;
  }
  effect {
    var l = my_asset.head(2).head(2);
    var k ?= l.nth(1) : "OUT_OF_BOUND";
    res := my_asset[k].value
  }
}

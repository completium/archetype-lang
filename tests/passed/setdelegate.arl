archetype setdelegate

variable res : option<string> = none

entry exec (k : key) {
  specification {
    s0: length(operations) = 1
  }
  effect {
    var kh : key_hash = hash_key(k);
    var okh : option<key_hash> = some(kh);
    var op : operation = set_delegate(okh);
    operations := prepend(operations, op)
  }
}

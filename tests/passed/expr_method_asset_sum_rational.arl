archetype expr_method_asset_sum_rational

asset my_asset identified by id {
  id : string;
  value : rational;
} initialized by {
  {"id0"; 0.1};
  {"id1"; 0.2};
  {"id2"; 0.5}
}

variable res : rational = 0

entry exec () {
  specification {
    s0: res = 0.8;
  }
  effect {
    res := my_asset.sum(value)
  }
}

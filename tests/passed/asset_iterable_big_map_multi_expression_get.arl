archetype asset_iterable_big_map_multi_expression_get

asset my_asset to iterable_big_map {
  id : string;
  value : nat = 0;
  data : bytes = 0x;
} initialized by {
  { "id0"; 0; 0x00 };
  { "id1"; 1; 0x01 };
  { "id2"; 2; 0x02 }
}

variable res : nat = 0

entry exec () {
  res := my_asset["id2"].value;
  dorequire(res = 2, "ko")
}

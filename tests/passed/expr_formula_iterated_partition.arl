archetype expr_formula_iterated_partition

asset o_asset identified by oid {
  oid : int;
}

asset my_asset identified by id {
  id : string;
  p : partition<o_asset>;
}

entry exec () {
  specification {
    postcondition p1 {
      true
      invariant for loop {
         iterated.isempty()
      }
    }
  }

  effect {
    var res : int = 0;
    for:loop i in my_asset["0"].p do
      res += 1;
    done
  }
}
archetype expr_method_asset_sum_partition

asset o_asset identified by oid {
  oid : int;
}

asset my_asset identified by id {
  id     : string;
  col    : partition<o_asset>;
} initialized by {
  {"id0"; []};
  {"id1"; []};
  {"id2"; []}
}

variable res : int = -1

entry exec () {
  res := my_asset["id0"].col.sum(oid);
  do_require (res = 0, "ko")
}

archetype expr_method_add_collection

asset o_asset identified by oid {
  oid : string;
} initialized by {
  {"oid"}
}

asset my_asset identified by id {
  id     : string;
  value  : int;
  col    : o_asset collection;
} initialized by {
  {"id0"; 0; []};
  {"id1"; 1; []};
  {"id2"; 2; []}
}

variable res : int = 0

action exec () {
  effect {
    my_asset.get("id0").col.add({"oid"})
  }
}

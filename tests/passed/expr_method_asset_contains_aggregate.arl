archetype expr_method_asset_contains_aggregate

asset o_asset identified by oid {
  oid : int;
}

asset my_asset identified by id {
  id     : string;
  col    : aggregate<o_asset>;
} initialized by {
  {"id0"; []};
  {"id1"; []};
  {"id2"; []}
}

variable res : bool = true

entry exec () {
  res := my_asset["id0"].col.contains(0);
}

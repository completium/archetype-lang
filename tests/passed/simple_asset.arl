archetype simple_asset

asset my_asset identified by id {
  id : string;
  value : int;
} initialized by {
  {"id0"; 0};
  {"id1"; 1};
  {"id2"; 2}
}

variable n : int = 0

entry add_asset () {
  my_asset.add({"id4"; 4})
}

entry remove_asset () {
  my_asset.remove("id0")
}

entry exec () {
  var v = my_asset["id1"].value;
  dorequire(v = 1, "ko");
  n := v;
}

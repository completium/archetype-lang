archetype test_list_mynth2

variable res : option<string> = none

function mynth(l : list<string>, idx : nat) : option<string> {
  var ll = l;
  var r : option<string> = none;
  var i : nat = 0;
  while : while_label i <= idx do
    match ll with
    | hd::tl -> begin
        r := some(hd);
        ll := tl;
        i += 1
      end
    | [] -> fail ("empty")
    end
  done;
  return r
}

entry exec () {
  var l : list<string> = ["1"; "2"; "3"];
  res := mynth(l, 1)
}

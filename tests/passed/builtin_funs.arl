archetype builtin_funs


action exec () {
  effect {
    var int_min : int = min(1, 2);
    var int_max : int = max(1, 2);

    var rat_min : rational = min(1 div 2, 1 div 3);
    var rat_max : rational = max(1 div 2, 1 div 3);

    var date_min : date = min(2019-01-01, 2020-01-01);
    var date_max : date = max(2019-01-01, 2020-01-01);

    var duration_min : duration = min(1h, 2s);
    var duration_max : duration = max(1h, 2s);

    var tez_min : tez = min(1tz, 2tz);
    var tez_max : tez = max(1tz, 2tz);

    require(true)
  }
}

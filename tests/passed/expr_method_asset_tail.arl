archetype expr_method_asset_tail

asset my_asset identified by id {
  id : string;
  value : int;
} initialized by {
  {"id0"; 0};
  {"id1"; 1};
  {"id2"; 2}
}

variable res : int = 0

entry exec () {
  specification {
    s0: res = 1;
  }
  effect {
    var l = my_asset.tail(2);
    var k ?= l.nth(0) : "OUT_OF_BOUND";
    res := my_asset[k].value
  }
}

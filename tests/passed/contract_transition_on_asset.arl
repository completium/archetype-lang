archetype contract_transition_on_asset

enum my_enum =
| First
| Second
| Third

asset my_asset {
  id : string;
  val : int;
} with states my_enum


transition exec (v : int) on (a : asset_key<my_asset>) {
  from First
  to Second
  when { true }
  with effect {
    my_asset.update(a, {val = v})
  }
}

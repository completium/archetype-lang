archetype test_removeif_part_2

asset my_asset identified by id {
  id     : nat;
  s      : string;
  b      : bool;
}

asset s_asset identified by sid {
  sid : string;
  c   : partition<my_asset>;
} initialized by {
  {"id0"; []};
  {"id1"; []};
  {"id2"; []}
}

entry exec (n : nat, %is : string) {
  s_asset["id0"].c.remove_if(the.id >= n or the.s = %is and the.b = false)
}

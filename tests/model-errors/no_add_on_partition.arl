archetype no_add_on_partition

asset my_asset identified by id {
  id : string;
  value : int;
}

asset other_asset identified by id_other {
  id_other : string;
  value_other : int;
  mypartition : my_asset partition;
}

entry main () {
  effect {
    my_asset.add ({ id = "id"; value = 0 }) (* fail *)
  }
}

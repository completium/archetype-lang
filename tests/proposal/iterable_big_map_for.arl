archetype iterable_big_map_for

variable my_map : iterable_big_map<string, bytes> = [ ("mystr0", 0x00) ; ("mystr1", 0x01) ; ("mystr2", 0x02) ; ("mystr3", 0x03) ]

entry exec () {
  var res : bytes = 0x;
  for (k, v) in my_map do
    res := concat(res, v)
  done;
  dorequire (res = 0x00010203, "ko")
}

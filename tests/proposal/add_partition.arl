archetype addupdate_partition

asset o_asset {
  oid : string;
  v : nat = 0;
}

asset my_asset {
  id     : string;
  value  : nat;
  col    : partition<o_asset>;
} initialized by {
  {"id0"; 0; []};
  {"id1"; 1; []};
  {"id2"; 2; []}
}

entry exec () {
  my_asset["id0"].col.add({ oid = "oid" ; v = 0 })
}

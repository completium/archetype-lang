archetype asset_types_getopt

asset abc {
  a : nat;
  b : string;
  c : bytes;
} initialized by {
  { 0; "mystr"; 0x }
}

entry get_value() {
  const ov : option<asset_value<abc>> = abc.get(0);
  const v = match ov with | some(v) -> v.b | none -> "" end;
  do_require(v = "mystr", "INVALID_VALUE")
}

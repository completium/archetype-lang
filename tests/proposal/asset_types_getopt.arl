archetype asset_types

asset my_asset {
  a : nat;
  b : string;
  c : bytes;
} initialized by {
  { 0; "mystr"; 0x }
}

entry get_value() {
  const ov : option<asset_value<my_asset>> = my_asset.getopt(0);
  const v = match ov with | some(v) -> v.b | none -> "" end;
  dorequire(v = "mystr", "INVALID_VALUE")
}

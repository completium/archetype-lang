archetype asset_update_with_basic_container_list

asset my_asset identified by id {
  id : string;
  v0 : nat;
  c  : list<nat>;
} initialized by {
  {"id0"; 0; []};
  {"id1"; 1; []};
  {"id2"; 2; []}
}

entry exec (n : nat) {
  my_asset.update("id1", {c += [0; 1; 2]});

  my_asset.update("id1", {c -= [2]})
}

# Unit
{
  storage unit;
  parameter (contract %export_ticket (list (ticket (pair nat (option bytes)))));
  code { NIL operation;
         DIG 1;
         UNPAIR;
         NIL (ticket (pair nat (option bytes)));
         PUSH nat 1;
         NIL operation;
         DUP 4;
         PUSH mutez 0;
         DIG 4;
         TRANSFER_TOKENS;
         CONS;
         DIP { DIG 3; DROP };
         DUG 3;
         DROP 3;
         DIG 1;
         PAIR };
}


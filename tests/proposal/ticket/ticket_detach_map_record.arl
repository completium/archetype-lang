archetype ticket_detach_map_record

variable input : map<nat, my_record> = []

variable output : option<ticket<string>> = none<ticket<string>>

entry init() {
  const nt ?= create_ticket("info", 1) : "ERROR";
  input := [(0, {"mystr"; nt})];
}

entry exec() {
  detach dt from m[0].t : "ERROR";
  output := some(dt)
}

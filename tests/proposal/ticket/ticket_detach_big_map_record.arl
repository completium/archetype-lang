archetype ticket_detach_big_map_record

record my_record {
  v : string;
  t : ticket<string>
}

variable input : big_map<nat, my_record> = []

variable output : option<ticket<string>> = none<ticket<string>>

entry init() {
  const nt ?= create_ticket("info", 1) : "ERROR";
  input := [(0, {"mystr"; nt})];
}

entry exec() {
  detach dt from input[0].t :: "ERROR";
  output := some(dt)
}

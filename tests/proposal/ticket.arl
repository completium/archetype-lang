archetype edo_ticket

variable rt : option<address * string * nat> = none
variable jt : option<ticket<string>> = none
variable st : option<ticket<string> * ticket<string>> = none

entry e0 () {
  const t = create_ticket ("mystring", 3);
  rt := some(read_ticket(t))
}

entry e1 () {
  const t1 = create_ticket ("mystring", 3);
  const t2 = create_ticket ("mystring", 5);
  jt := join_tickets(t1, t2)
}

entry e2 () {
  const t = create_ticket ("mystring", 3);
  st := split_ticket(t, 1, 2)
}
